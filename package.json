{
  "name": "branches_front_end_private_from_scratch",
  "version": "1.0.0",
  "scripts": {
    "pre-dev": "time docker build --file=./Dockerfile --tag=branches_front_end_from_scratch ./",
    "dev": "cross-env app_env=development docker run --name=branches_front_end_from_scratch_$(date '+%Y_%m_%d_%H_%M_%S') -it -p 8080:8080 -v \"$(pwd)\":/frontend branches_front_end_from_scratch",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "cross-env NODE_ENV=test ava-ts *.spec.ts app/*.spec.ts app/components/**/*.spec.ts app/core/*.spec.ts app/loaders/**/*.spec.ts app/objects/**/*.spec.ts app/objects/**/**/*.spec.ts app/objects/*.spec.ts --verbose --color",
    "test-some": "cross-env NODE_ENV=test ava-ts --color",
    "test:watch": "cross-env NODE_ENV=test ava-ts *.spec.ts app/*.spec.ts app/components/**/*.spec.ts app/core/*.spec.ts app/loaders/**/*.spec.ts app/objects/**/*.spec.ts app/objects/**/**/*.spec.ts app/objects/*.spec.ts --verbose --color --watch",
    "coverage": "gulp coverage",
    "deploy": "sh .deploy.sh",
    "publish-coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.10"
  },
  "devDependencies": {
    "@types/node": "^8.0.50",
    "awesome-typescript-loader": "^5.2.1",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.4.1",
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "typescript": "^2.6.2",
    "ts-loader": "^5.2.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-stream": "^4.0.0"
  }
}
